<!-- Base -->

<table{{ attributes }}>
  {{ caption }}
  {{ colgroups }}
  {{ header }}
  {{ rows }}
</table>

<!-- Clarification A -->

<table{{ attributes }}>
  <caption{{ caption.attributes }}>
    {{ caption.inner }}
  </caption>
  <colgroup{{ colgroup.attributes }}>
    {{ colgroup.inner }}
  </colgroup>
  <thead{{ header.attributes }}>
    {{ header.inner }}
  </thead>
  <tbody{{rows.attributes}}>
    {{ rows.inner }}
  </tbody>
</table>

<!-- Clarification B -->

<table{{ attributes }}>
  <caption{{ caption.attributes }}>
    {{ caption.inner }}
  </caption>
  <colgroup{{ colgroup.attributes }}>
  {% for col in colgroup.inner %}
    {{ col }}
  {% endfor %}
  </colgroup>
  <thead{{ header.attributes }}>
  {% for row in header.inner %}
    {{ row }}
  {% endfor %}
  </thead>
  <tbody{{ rows.attributes }}>
  {% for row in rows.inner %}
    {{ row }}
  {% endfor %}
  </tbody>
</table>

